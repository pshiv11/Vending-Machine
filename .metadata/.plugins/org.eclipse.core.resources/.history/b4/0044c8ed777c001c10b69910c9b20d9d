package vendingmachine.dto;

import java.math.BigDecimal;

public class Item {
	
	private int id;
	private String name;
	private BigDecimal price;
	private String quantity;
	
	
	public Item(String name, BigDecimal price, String quantity) {
		this.name = name;
		this.price = price;
		this.quantity = quantity;
	}
	
	
	
	

}
